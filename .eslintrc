{
    "extends": "eslint:recommended",
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "semi": "error",
				"indent": ["error", 2],
				"no-console": "off"
    },
		"globals": {
        "sails": false
		}
}
